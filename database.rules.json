{
  "rules": {
    ".read": "true",
    "meta": {
    },
    "users": {
      "$uid": {
        ".write": "$uid === auth.uid",
        "serial": {
          ".write": "false"
        }
      }
    },
    "photos": {
      "$photo_id": {
        ".write": "$uid === auth.uid",
          "serial": {
            ".write": "false"
          }
      }
    },
    "likes": {
      "$like_id": {
        ".write": "$uid === auth.uid",
          "serial": {
            ".write": "false"
          }
      }
    }
  }
}
