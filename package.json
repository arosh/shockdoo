{
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^4.9.0",
    "flexboxgrid": "^6.3.1",
    "font-awesome": "^4.7.0",
    "lodash.range": "^3.2.0",
    "material-design-lite": "^1.3.0",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-media": "^1.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "sha.js": "^2.4.10"
  },
  "devDependencies": {
    "@google-cloud/functions-emulator": "latest",
    "@storybook/react": "^3.3.10",
    "firebase-tools": "^3.17.3",
    "flow-bin": "^0.64.0",
    "node-sass-chokidar": "^0.0.3",
    "prettier": "^1.10.2",
    "react-scripts": "1.1.0",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer build/static/js/main.*",
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "deploy:firestore": "firebase deploy --only firestore:rules",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "yarn build && firebase deploy --only hosting",
    "deploy:storage": "firebase deploy --only storage",
    "flow": "flow",
    "format": "prettier --write \"src/**/*.js\" \"functions/index.js\"",
    "serve": "firebase serve --only hosting,functions",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public"
  },
  "proxy": {
    "/__": {
      "target": "http://localhost:5000"
    },
    "/api": {
      "target": "http://localhost:5000"
    }
  }
}
